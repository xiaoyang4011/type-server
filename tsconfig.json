{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "removeComments": false,
    "sourceMap": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
      "src/**/*.ts"
  ]
}